{
  "name": "queo/doctrine-filter",
  "description": "Easy filter for Doctrine ORM",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Uhlmann",
      "email": "tho.uhlmann@gmail.com"
    }
  ],
  "require": {
    "doctrine/orm": ">=2.4.5",
    "pagerfanta/pagerfanta": "^1.0",
    "symfony/options-resolver": "^2.7|^3.0|^4.0|^5.4|^6.0",
    "doctrine/annotations": "^1.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "doctrine/data-fixtures": "^1.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BiteCodes\\DoctrineFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BiteCodes\\DoctrineFilter\\Tests\\": "tests/"
    }
  }
}
